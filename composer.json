{
    "name": "firefly-iii/api-support-classes",
    "type": "library",
    "description": "API support classes",
    "keywords": [
        "firefly-iii",
        "api-support-classes"
    ],
    "homepage": "https://github.com/firefly-iii/api-support-classes",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "James Cole",
            "email": "james@firefly-iii.org",
            "homepage": "https://github.com/JC5",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/support": "^6.17",
      "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GrumpyDictator\\FFIIIApiSupport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrumpyDictator\\FFIIIApiSupport\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
